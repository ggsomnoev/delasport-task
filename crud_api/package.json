  "license": "ISC",
  "description": "",
  "dependencies": {
    "cors": "^2.8.5",
    "crud_api": "file:",
    "dotenv": "^16.4.7",
    "eslint-plugin-prettier": "^5.2.3",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.5",
    "sequelize-cli": "^6.6.2",
    "uuid": "^11.0.5",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
